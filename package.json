{
  "name": "datatable-vue",
  "version": "0.1.0",
  "description": "A flexible, configurable grid component library for Vue 3 applications with support for both cursor and page-based pagination, custom data providers, and extensive customization options.",
  "author": "Dima Popov",
  "license": "MIT",
  "type": "module",
  "main": "./dist/datatable-vue.umd.cjs",
  "module": "./dist/datatable-vue.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/datatable-vue.js",
      "require": "./dist/datatable-vue.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "vue",
    "vue3",
    "datatable",
    "grid",
    "table",
    "pagination",
    "data-provider",
    "typescript",
    "cursor-pagination",
    "page-pagination"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/einhasad/vue-datatable.git"
  },
  "bugs": {
    "url": "https://github.com/einhasad/vue-datatable/issues"
  },
  "homepage": "https://github.com/einhasad/vue-datatable#readme",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run postbuild",
    "postbuild": "rm -rf dist/node_modules dist/vite.config.d.ts dist/vite.config.d.ts.map dist/__VLS_types.d.ts",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "vue-router": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
